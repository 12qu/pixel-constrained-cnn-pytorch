{
  "name": "mnist_model",
  "dataset": "mnist",
  "resize": 28,
  "crop": 28,
  "grayscale": true,
  "batch_size": 64,
  "constrained": false,
  "num_colors": 2,
  "filter_size": 5,
  "depth": 16,
  "num_filters_cond": 32,
  "num_filters_prior": 32,
  "lr": 4e-4,
  "epochs": 5,
  "mask_descriptor": ["random_rect", [10, 10]],
  "num_conds": 4,
  "num_samples": 64,
  "weight_cond_logits_loss": 1.0,
  "weight_prior_logits_loss": 0.0
}
